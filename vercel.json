{
  "functions": {
    "speed/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/speed/(.*)", "dest": "/speed/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "env": {
    "NOW_PHP_FOO": "bar3"
  },
  "build": {
    "env": {
      "NOW_PHP_DEBUG": "1"
    }
  }
}
