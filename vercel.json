{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    { "src": "/", "dest": "/index.php" }
    { "src": "/speed", "dest": "speed/index.html" }
    { "src": "/peed/backend/empty.php", "dest": "speed/backend/empty.php" }
    { "src": "/speed/backend/garbage.php", "dest": "speed/backend/garbage.php" }
    { "src": "/speed/backend/getIP.php", "dest": "speed/backend/getIP.php" }
    { "src": "/speed/backend/getIP_ipInfo_apikey.php.ph", "dest": "speed/backend/getIP_ipInfo_apikey.php.php" }
    { "src": "/speedtest_worker", "dest": "speed/speedtest_worker.js" }
    { "src": "/speedtest", "dest": "speed/speedtest.js" }
    
  ],
  "build": {
    "env": {
      "NOW_PHP_DEBUG": "1"
    }
  }
}
